{
  "name": "poglin-syntax",
  "displayName": "Poglin Syntax Highlight",
  "description": "Syntax highlighting for the Poglin programming language (.pog files).",
  "version": "0.0.2",
  "publisher": "GABESANCTI",
  "author": {
    "name": "Gabe Sancti",
    "email": "gabesancti@gmail.com"
  },
  "license": "MIT",
  "engines": {
    "vscode": "^1.80.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "keywords": [
    "poglin",
    "syntax",
    "highlight",
    "language",
    "compilers"
  ],
  "homepage": "https://github.com/GABESANCTI/poglin-syntax#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/GABESANCTI/poglin-syntax.git"
  },
  "bugs": {
    "url": "https://github.com/GABESANCTI/poglin-syntax/issues"
  },
  "icon": "images/icon.png",
  "contributes": {
    "languages": [{
      "id": "poglin",
      "aliases": ["Poglin", "poglin"],
      "extensions": [".pog"],
      "configuration": "./language-configuration.json"
    }],
    "grammars": [{
      "language": "poglin",
      "scopeName": "source.poglin",
      "path": "./syntaxes/poglin.tmLanguage.json"
    }]
  },
  "devDependencies": {
    "@types/vscode": "^1.80.0",
    "@types/glob": "^8.1.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "^18.17.0",
    "eslint": "^8.45.0",
    "typescript": "^5.1.6",
    "@vscode/test-cli": "^0.0.4",
    "@vscode/test-electron": "^2.3.2"
  }
}